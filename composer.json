{
  "name": "previousnext/skpr-config",
  "description": "Reads skipper configuration files into environment variables.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "require-dev": {
    "drupal/coder": "~8.2",
    "phpunit/phpunit": "~6.5"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PNX\\SkprConfig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PNX\\SkprConfig\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "~7.0"
  }
}
